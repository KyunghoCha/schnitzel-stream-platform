# Core pipeline overrides (mapped via apply_env_overrides)
AI_EVENTS_POST_URL=http://localhost:8080/api/events
AI_INGEST_FPS_LIMIT=10
AI_SOURCE_TYPE=file
# Optional custom input source plugin (module:ClassName)
# AI_SOURCE_ADAPTER=ai.plugins.ros2.image_source:Ros2ImageSource
# Sensor lane (P1): enabled sensor packets are attached to emitted vision events.
AI_SENSOR_ENABLED=false
# AI_SENSOR_TYPE=ultrasonic
# AI_SENSOR_ADAPTER=ai.plugins.sensors.fake_ultrasonic:FakeUltrasonicSensorSource
# AI_SENSOR_ADAPTERS=ai.plugins.sensors.fake_ultrasonic:FakeUltrasonicSensorSource,ai.plugins.sensors.serial_ultrasonic:SerialUltrasonicSensorSource
# AI_SENSOR_TOPIC=/sensors/ultrasonic/front
AI_SENSOR_QUEUE_SIZE=256
AI_SENSOR_TIME_WINDOW_MS=300
# Emit independent SENSOR_EVENT payloads from sensor lane
AI_SENSOR_EMIT_EVENTS=false
# Emit additional FUSED_EVENT payloads (vision + nearest sensor)
AI_SENSOR_EMIT_FUSED_EVENTS=false
# Fake sensor plugin options (for local/no-hardware test path)
AI_FAKE_SENSOR_ID=ultrasonic-front-01
AI_FAKE_SENSOR_INTERVAL_SEC=0.05
AI_EVENTS_SNAPSHOT_BASE_DIR=/tmp/snapshots
AI_EVENTS_SNAPSHOT_PUBLIC_PREFIX=/tmp/snapshots
# Optional custom emitter plugin (module:ClassName)
# AI_EVENTS_EMITTER_ADAPTER=ai.plugins.ros2.event_emitter:Ros2EventEmitter
AI_MODEL_MODE=real
# NOTE: default custom adapter is a template and fails fast until implemented.
AI_MODEL_ADAPTER=ai.vision.adapters.custom_adapter:CustomModelAdapter
AI_ZONES_SOURCE=api
AI_RTSP_TRANSPORT=tcp

# ROS2 plugin options (used only when source/emitter adapter points to ROS2 plugins)
AI_ROS2_SOURCE_TOPIC=/camera/image_raw/compressed
AI_ROS2_SOURCE_MSG_TYPE=compressed
AI_ROS2_SOURCE_NODE_NAME=ai_frame_source
AI_ROS2_SOURCE_QOS_DEPTH=10
AI_ROS2_SOURCE_READ_TIMEOUT_SEC=1.0
AI_ROS2_SOURCE_FPS_HINT=0
AI_ROS2_EVENT_TOPIC=/ai/events
AI_ROS2_EMITTER_NODE_NAME=ai_event_emitter
AI_ROS2_EMITTER_QOS_DEPTH=100

# Logging overrides (AI_LOG_* and AI_LOGGING_* both supported; AI_LOG_* takes precedence at runtime)
AI_LOGGING_LEVEL=INFO
AI_LOGGING_FORMAT=plain
AI_LOG_LEVEL=INFO
AI_LOG_FORMAT=plain
AI_LOG_MAX_BYTES=10485760
AI_LOG_BACKUP_COUNT=5

# Model / Tracker (adapter-level env vars; used by from_env())
MODEL_CLASS_MAP_PATH=configs/model_class_map.yaml
TRACKER_TYPE=none
TRACKER_MAX_AGE=30
TRACKER_MIN_HITS=1
TRACKER_IOU=0.3
YOLO_MODEL_PATH=/path/to/model.pt
YOLO_CONF=0.25
ONNX_MODEL_PATH=/path/to/model.onnx
ONNX_CONF=0.25
ONNX_IOU=0.45
ONNX_PROVIDERS=CUDAExecutionProvider,CPUExecutionProvider

# Mock backend (host E2E)
MOCK_BACKEND_HOST=127.0.0.1
MOCK_BACKEND_PORT=18080
